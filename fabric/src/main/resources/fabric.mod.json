{
    "schemaVersion": 1,
    "id": "infernalexp",
    "version": "${version}",
    "name": "Infernal Expansion",
    "description": "Expanding the Nether beyond its official update with tons of new mobs & blocks, all while staying true to the Vanilla feel!",
    "authors": [
        "Lyof, Nekomaster, evanbones"
    ],
    "contact": {
        "homepage": "https://infernalstudios.org/",
        "sources": "https://github.com/infernalstudios/Infernal-Expansion/",
        "issues": "https://github.com/infernalstudios/Infernal-Expansion/issues"
    },
    "license": "AGPL-3.0",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "com.infernalstudios.infernalexp.fabric.InfernalExpansion"
        ],
        "client": [
            "com.infernalstudios.infernalexp.fabric.client.InfernalExpansionFabricClient"
        ],
        "terrablender": [
            "com.infernalstudios.infernalexp.fabric.compat.FabricTerraBlenderCompat"
        ],
        "fabric-datagen": [
            "com.infernalstudios.infernalexp.fabric.datagen.IEDataGenerator"
        ]
    },
    "mixins": [
        "infernalexp.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15.10",
        "minecraft": "~1.20.1",
        "java": ">=17",
        "fabric-api": "*"
    }
}
  